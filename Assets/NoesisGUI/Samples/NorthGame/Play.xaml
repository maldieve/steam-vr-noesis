                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                <EasingDoubleKeyFrame KeyTime="0:0:3.5" Value="2"/>
                <EasingDoubleKeyFrame KeyTime="0:0:5.5" Value="1.5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:7.5" Value="2.5"/>
