            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="TopMenuDimGlow">
                <EasingDoubleKeyFrame KeyTime="0" Value="-1200"/>
                <EasingDoubleKeyFrame KeyTime="0:0:11" Value="1300">
