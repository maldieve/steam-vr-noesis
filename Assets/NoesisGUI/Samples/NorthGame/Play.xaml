    <Grid x:Name="LayoutRoot" RenderTransformOrigin="0.5,-0.1" IsEnabled="False">

        <b:Interaction.Triggers>
            <b:EventTrigger EventName="Loaded">
                <b:ControlStoryboardAction Storyboard="{StaticResource TopInfoAnim}"/>
            </b:EventTrigger>
            <b:DataTrigger Binding="{Binding State}" Value="{x:Static local:State.Play}">
                <b:ControlStoryboardAction Storyboard="{StaticResource FadeIn}"/>
            </b:DataTrigger>
            <b:DataTrigger Binding="{Binding State}" Comparison="NotEqual" Value="{x:Static local:State.Play}">
                <b:ControlStoryboardAction Storyboard="{StaticResource FadeOut}"/>
            </b:DataTrigger>
            <b:DataTrigger Binding="{Binding IsEnabled, ElementName=LayoutRoot}" Value="True">
                <b:InvokeCommandAction Command="{Binding FadeCompletedCommand}"/>
                <b:ChangePropertyAction PropertyName="SelectedIndex" TargetName="Navigation" Value="0"/>
                <noesis:SetFocusAction TargetName="Navigation"/>
            </b:DataTrigger>
            <b:DataTrigger Binding="{Binding PlayScreenDirection}" Value="{x:Static local:PlayScreenDirection.Next}">
                <b:ControlStoryboardAction Storyboard="{StaticResource SlideNext}"/>
            </b:DataTrigger>
            <b:DataTrigger Binding="{Binding PlayScreenDirection}" Value="{x:Static local:PlayScreenDirection.Prev}">
                <b:ControlStoryboardAction Storyboard="{StaticResource SlidePrev}"/>
            </b:DataTrigger>
        </b:Interaction.Triggers>

        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <Grid KeyboardNavigation.DirectionalNavigation="Contained">
            <Grid.RowDefinitions>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="100*"/>
                <RowDefinition Height="470*"/>
                <RowDefinition Height="30*"/>
            </Grid.RowDefinitions>
            <Grid x:Name="MenuButtonGrid" Grid.Row="0" RenderTransformOrigin="0.5,0.5">
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>
